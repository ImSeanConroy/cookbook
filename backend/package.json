    "express": "^5.1.0",
    "pg": "^8.16.2"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.19",
    "@types/pg": "^8.15.4",
    "node-pg-migrate": "^8.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
