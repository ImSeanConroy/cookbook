{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev --files src/index.ts",
    "build": "tsc && cp /package.json ./dist",
    "start": "node dist/index.js",
    "migrate:up": "node-pg-migrate --migration-file-language sql up",
